<%= c 'header' %>
<%= c 'wrapper' do %>
  <%= c 'side' do %>
    <%= c 'username', user_name: current_consumer.instagram.user_name, picture_url: current_consumer.instagram.profile_pict %>
    <%= c 'bio', user: current_consumer %>
    <%= c 'button', additionnal_classes: "secondary" do %>
      > My Instagram Report
    <% end %>
    <%= c 'community' %>


  <% end %>
  <%= c 'main' do %>
    <%= c 'feed' do %>
      <% @campaigns.each do |campaign| %>
        <%= c 'campaign', campaign: campaign %>
      <% end %>
    <% end %>
  <% end %>
<% end %>
