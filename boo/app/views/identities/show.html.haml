= image_tag @identity.picture_url, class: "avatar"
%h1= t('influencer_information')
%h2= t('last_instagram_posts')

= @identity.posts.order(created_at: :desc).limit(4).each do |post|
  = ui_post_card image_url: post.profile_pict, likes_count: post.likes_count, comments_count: post.comments_count, trend_percent: post.trend_percent

%h2= t('instagram_statistics')
%h3= t('posts_habits')

